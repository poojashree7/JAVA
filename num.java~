import java.util.Scanner;
public class num{
  public static void main(String args[]){
    Scanner Sc= new Scanner(System.in);
    int n=Sc.nextInt();
    if (n>0){
      System.out.println("positive number");
    }
    else if (n==0){
      System.out.println("given num is zero");
    }
    else{
      System.out.println("negative number");
    }
   
    
  }
}